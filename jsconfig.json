{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "vueCompilerOptions": {
    "skipTemplateCodegen": true,
    "extensions": [".vue"],
    "target": 2
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
